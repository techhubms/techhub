{
    "Logging": {
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "File": {
            "LogLevel": {
                "Default": "Information"
            },
            "Path": "/workspaces/techhub/.tmp/logs/api-prod.log"
        }
    },
    "AppSettings": {
        "Content": {
            "CollectionsPath": "/workspaces/techhub/collections"
        }
    },
    "Database": {
        "Provider": "PostgreSQL",
        "ConnectionString": "Host=localhost;Port=5432;Database=techhub;Username=techhub;Password=prod"
    },
    "ContentSync": {
        "Enabled": true,
        "ForceFullSync": false,
        "MaxParallelFiles": 10
    },
    "Cors": {
        "AllowedOrigins": [
            "https://localhost:5003"
        ]
    }
}