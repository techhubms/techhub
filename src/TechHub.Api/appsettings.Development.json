{
  "Logging": {
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "File": {
      "LogLevel": {
        "Default": "Information"
      },
      "Path": "/workspaces/techhub/.tmp/logs/api-dev.log"
    }
  },
  "AppSettings": {
    "Content": {
      "CollectionsPath": "/workspaces/techhub/collections",
      "SectionsConfigPath": "/workspaces/techhub/_data/sections.json",
      "Timezone": "Europe/Brussels"
    },
    "Caching": {
      "ContentAbsoluteExpirationMinutes": 5,
      "ContentSlidingExpirationMinutes": 2,
      "ApiResponseAbsoluteExpirationMinutes": 5,
      "EnableOutputCaching": false
    },
    "Seo": {
      "BaseUrl": "https://localhost:7000",
      "SiteTitle": "Microsoft Tech Hub (Dev)",
      "SiteDescription": "Development instance"
    },
    "Performance": {
      "EnableCompression": false,
      "EnableHttp2": true,
      "EnableHttp3": false,
      "MaxConcurrentRequests": 100
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "https://localhost:7001",
      "http://localhost:5000"
    ]
  }
}