{
  "Logging": {
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "File": {
      "LogLevel": {
        "Default": "Information",
        "System.Net.Http.HttpClient": "Warning",
        "Polly": "Warning"
      },
      "Path": "/workspaces/techhub/.tmp/logs/api-dev.log"
    }
  },
  "AppSettings": {
    "Content": {
      "CollectionsPath": "/workspaces/techhub/collections"
    }
  },
  "Database": {
    "Provider": "SQLite",
    "ConnectionString": "Data Source=/workspaces/techhub/techhub.db"
  },
  "ContentSync": {
    "Enabled": true,
    "ForceFullSync": false,
    "MaxParallelFiles": 10
  },
  "Cors": {
    "AllowedOrigins": [
      "https://localhost:5003"
    ]
  }
}